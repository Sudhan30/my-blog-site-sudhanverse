{
  "dashboard": {
    "id": null,
    "title": "Blog Analytics - Comprehensive Dashboard",
    "tags": ["blog", "analytics", "comprehensive"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(blog_page_views_total)",
            "legendFormat": "Total Views"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "blog_active_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Bounce Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "blog_bounce_rate",
            "legendFormat": "Bounce Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.6
                },
                {
                  "color": "red",
                  "value": 0.8
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Avg Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "blog_average_session_duration_seconds",
            "legendFormat": "Avg Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Page Views Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(blog_page_views_total[5m])) by (page)",
            "legendFormat": "{{page}}"
          }
        ],
        "yAxes": [
          {
            "label": "Views/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Top Pages",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(blog_page_views_total[1h])) by (page))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "User Engagement Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(blog_clicks_total[5m]))",
            "legendFormat": "Clicks/sec"
          },
          {
            "expr": "sum(rate(blog_comments_total[5m]))",
            "legendFormat": "Comments/sec"
          },
          {
            "expr": "sum(rate(blog_feedback_total[5m]))",
            "legendFormat": "Feedback/sec"
          },
          {
            "expr": "sum(rate(blog_likes_total[5m]))",
            "legendFormat": "Likes/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Device & Browser Breakdown",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(blog_page_views_total) by (device_type)",
            "legendFormat": "{{device_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Browser Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(blog_page_views_total) by (browser)",
            "legendFormat": "{{browser}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 10,
        "title": "Scroll Depth Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(blog_scroll_depth_bucket[5m])) by (le))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.75, sum(rate(blog_scroll_depth_bucket[5m])) by (le))",
            "legendFormat": "75th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, sum(rate(blog_scroll_depth_bucket[5m])) by (le))",
            "legendFormat": "90th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Scroll Depth %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Time on Page Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(blog_time_on_page_seconds_bucket[5m])) by (le))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.75, sum(rate(blog_time_on_page_seconds_bucket[5m])) by (le))",
            "legendFormat": "75th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, sum(rate(blog_time_on_page_seconds_bucket[5m])) by (le))",
            "legendFormat": "90th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Time (seconds)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 12,
        "title": "Reading Progress by Post",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(blog_reading_progress_percent) by (post_id)",
            "legendFormat": "Post {{post_id}}"
          }
        ],
        "yAxes": [
          {
            "label": "Progress %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 13,
        "title": "Newsletter Signups",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_newsletter_signups_total[1d]))",
            "legendFormat": "Signups/day"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 14,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_errors_total[5m]))",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 32
        }
      },
      {
        "id": 15,
        "title": "Custom Events",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(blog_custom_events_total[1h])) by (event_name))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 16,
        "title": "Session Analytics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(blog_sessions_total[5m]))",
            "legendFormat": "New Sessions/sec"
          },
          {
            "expr": "blog_bounce_rate",
            "legendFormat": "Bounce Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "type": "tags",
          "tags": ["deployment"]
        }
      ]
    }
  }
}
