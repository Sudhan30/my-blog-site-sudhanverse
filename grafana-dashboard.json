{
  "dashboard": {
    "id": null,
    "title": "Blog Analytics Dashboard",
    "tags": ["blog", "analytics"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_page_views_total[5m]))",
            "legendFormat": "Page Views/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": []
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "blog_active_users{time_window=\"5m\"}",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Comments",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_comments_total[1h]))",
            "legendFormat": "Comments/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Feedback Rating Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(blog_feedback_total) by (rating)",
            "legendFormat": "Rating {{rating}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Page Views Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(blog_page_views_total[5m])) by (page)",
            "legendFormat": "{{page}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Top Pages",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(rate(blog_page_views_total[1h])) by (page))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "User Engagement Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(blog_likes_total[5m]))",
            "legendFormat": "Likes/sec"
          },
          {
            "expr": "sum(rate(blog_comments_total[5m]))",
            "legendFormat": "Comments/sec"
          },
          {
            "expr": "sum(rate(blog_feedback_total[5m]))",
            "legendFormat": "Feedback/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Page Load Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(blog_page_load_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(blog_page_load_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(blog_page_load_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Reading Progress",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(blog_reading_progress_percent) by (post_id)",
            "legendFormat": "Post {{post_id}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Newsletter Signups",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_newsletter_signups_total[1d]))",
            "legendFormat": "Signups/day"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(blog_errors_total[5m]))",
            "legendFormat": "Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
